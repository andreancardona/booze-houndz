<div class="new_create_review">Create A Review</div>
<div class="create_review_button">
<%= render "errors/error_messages"%>
<%= form_for @review  do |f| %>
  <%= f.label :content %>
  <%= f.text_area :content%>

  <%= f.label :rating %>
  <%= f.number_field :rating%>
  <%= f.hidden_field :liquor_store_id,value: @liquor_store.id %>
  <%= f.submit %>
<% end %>
</div>
